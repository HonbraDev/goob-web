{
    "github": {
        "silent": true
    },
    "redirects": [
        {
            "source": "/img/cats/:path",
            "destination": "/img/cta/:path",
            "permanent": true
        },
        {
            "source": "/cat-api",
            "destination": "/cta-api",
            "permanent": true
        },
        {
            "source": "/blog/cat-api",
            "destination": "/cta-api",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/.well-known/matrix/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                }
            ]
        }
    ]
}